{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 1,
        "no-empty": 0,
        "no-irregular-whitespace": 0,
        "indent": [0, 4 ],
        "object-shorthand": [ 2, "always" ],
        "no-await-in-loop" : 1,
        "no-param-reassign" : 0,
        "no-plusplus" : 0,
        "max-len" : 0,
        "func-names" : 0,
        "consistent-return" : 0,
        "camelcase" : 0,
        "no-underscore-dangle" : 0,
        "eqeqeq" : 0,
        "radix":0,
        "no-else-return" : 0,
        "prefer-destructuring" : 0,
        "no-shadow" : 0,
        "no-inner-declarations" : 0,
        "no-continue" : 0,
        "no-self-assign" : 0,
        "no-self-compare" : 0
    }
}
